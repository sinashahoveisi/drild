{
  "semi": true,
  "printWidth": 120,
  "tabWidth": 2,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "singleQuote": true,
  "useTabs": false,
  "Parser": "typescript",
  "requirePragma" : true,
  "insertPragma" : true,
  "proseWrap" : "preserve",
  "htmlWhitespaceSensitivity" : "css",
  "trailingComma": "none",
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "arrowParens": "always"
}
